 <section id="content">

  <nav>
		<i class='bx bx-menu'></i>
		<form action="#">
			<div class="form-input"></div>
		</form>
		<input type="checkbox" id="switch-mode" hidden>
		<label for="switch-mode" class="switch-mode"></label>
	</nav>
    <!-- Container Div -->
  <main>
	<div class="head-title">
		<h1 style="color: #3C91E6;">Edit Users</h1>
	</div>
	
	<form action="/admin/users/edit/<%= user.ID %>" method="POST">
        <div>
			<label for="username" style="color: #3C91E6;">Username:</label>
          <input type="text" id="username" name="username" value="<%= user.Username %>">
        </div>
        <div>
			<label for="password" style="color: #3C91E6;">Password:</label>
          <input type="password" id="password" name="password">
        </div>
        <div>
          <input type="submit" value="Update User">
        </div>
      </form>
  </main>
 </section>
<script>
	const switchMode = document.getElementById('switch-mode');
	const searchButton = document.querySelector('#content nav form .form-input button');
	const searchButtonIcon = document.querySelector('#content nav form .form-input button .bx');
	const searchForm = document.querySelector('#content nav form');
	const menuBar = document.querySelector('#content nav .bx.bx-menu');
	const sidebar = document.getElementById('sidebar');
	// Check if dark mode preference exists in local storage
const isDarkMode = localStorage.getItem('darkMode') === 'true';

// Set the initial dark mode state
if (isDarkMode) {
  document.body.classList.add('dark');
  switchMode.checked = true;
}

// Function to handle the dark mode switch
function handleDarkMode() {
  if (this.checked) {
    document.body.classList.add('dark');
    localStorage.setItem('darkMode', 'true');
  } else {
    document.body.classList.remove('dark');
    localStorage.setItem('darkMode', 'false');
  }
}

// Event listener for dark mode switch
switchMode.addEventListener('change', handleDarkMode);

// Event listener for window resize
window.addEventListener('resize', function () {
  if (this.innerWidth > 576) {
    searchButtonIcon.classList.replace('bx-x', 'bx-search');
    searchForm.classList.remove('show');
  }
});

// Event listener for menu bar
menuBar.addEventListener('click', function () {
  sidebar.classList.toggle('hide');
});


</script>
